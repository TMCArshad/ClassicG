    <script src="{!URLFOR($Resource.GEMS, 'js/jquery.min.js')}"></script>

    <script>

        // ie console, polyfill
        window.console = window.console || {log: function () {}};

        $(function(){
            if ("ActiveXObject" in window) {

                enableCardReadBtn(true);
                try {
                    //cardReader = Initialize();
                }catch (ex){
                    console.log('card reader not able to initialized..' + ex);
                }
            }
            else {
                //cardReadStatus = 'failed';
                enableCardReadBtn(false);
                activeX_NotSupported();
            }
        })

        function enableCardReadBtn(enable){
            if(enable)
                $('input[id$="readCardDetailsBtn"]').prop("disabled", false).removeClass('btnDisabled').addClass('btn');
            else
                $('input[id$="readCardDetailsBtn"]').prop("disabled", true).addClass('btnDisabled').removeClass('btn');
        }

        function disableFields(enable){
            var dataRows = document.getElementsByClassName('dataRow');
            for(var row =0; row < dataRows.length; row++){
                var rowElem = dataRows[row];

                var rowCells = rowElem.children;
                if(rowCells.length == 4){

                    if(enable)
                        $(rowCells[3]).find('input,select').prop('disabled', true);
                    else
                        $(rowCells[3]).find('input,select').prop('disabled', false);
                }
            }
        }


        document.addEventListener('DOMContentLoaded', function(){

            //document.getElementById("{!$Component.GEMS_EmiratesIDSFReader.emiratesForm.cardDetailsContainer.buttonsSet.bottom.syncUpdateBtn}").disabled=true;
            //$('input[id$="syncUpdateBtn"]')

            //document.getElementsByTagName('head')[0].appendChild('<meta http-equiv="X-UA-Compatible" content="IE=edge" />');
            var meta = document.createElement('meta');
            meta.httpEquiv = "X-UA-Compatible";
            meta.content = "IE=edge";
            document.getElementsByTagName('head')[0].appendChild(meta);

            var dataRows = document.getElementsByClassName('dataRow');
            for(var row =0; row < dataRows.length; row++){
                var rowElem = dataRows[row];

                rowElem.onmouseover = function(){
                    if (window.hiOn){
                        hiOn(this);
                    }
                };

                rowElem.onmouseout = function(row){
                    if (window.hiOff){
                        hiOff(this);
                    }
                };
                rowElem.onblur = function(){
                    if (window.hiOff){hiOff(this);}
                }
                rowElem.onfocus = function(){
                    if (window.hiOn){hiOn(this);}
                }
            }

            // Disable fields
            disableFields(true);
        })


        var card_details = {},
                cardReader;
        function read_card_details(){


            try {
                //if initialization is successful
                updateCardReadStatus('{!CARD_READ_STATUS_STARTED}');
                if(cardReader !== '') //re-init
                    cardReader = Initialize();

                if(cardReader == '') {

                    //$(document.getElementById('{!$Component.emiratesForm.cardDetailsContainer.cardReadStatusHiddenBtn}')).val({!CARD_READ_STATUS_STARTED});

                    console.log('Staring : Card Reading status ==>' + $(document.getElementById('{!$Component.emiratesForm.cardDetailsContainer.cardReadStatusHiddenBtn}')).val())

                    var photo_data = DisplayPhotography(),
                            public_data = DisplayPublicData(),
                            public_dataEx = DisplayPublicDataEx();

                    $.extend(card_details, photo_data, public_data, public_dataEx);
                    photo_data = null;
                    public_data = null;
                    public_dataEx = null;

                    $('input[id$="syncUpdateBtn"]').prop("disabled", false).removeClass('btnDisabled').addClass('btn');
                    //console.log(JSON.stringify(card_details));
                    DisplayEmiratesIdInfo();

                    //$(document.getElementById('{!$Component.emiratesForm.cardDetailsContainer.cardReadStatusHiddenBtn}')).val({!CARD_READ_STATUS_COMPLETED});

                    console.log('Finished : Card Reading status ==>' + $(document.getElementById('{!$Component.emiratesForm.cardDetailsContainer.cardReadStatusHiddenBtn}')).val())

                }else {
                    console.log('EIDA Toolkit initialization failed! ' + cardReader);
                }
            }catch(ex){
                alert('EIDA Toolkit initialization failed! \nDetail: ' + cardReader);
            }finally {
                updateCardReadStatus('{!CARD_READ_STATUS_COMPLETED}');
            }

        }

        function  updateCardReadStatus(status){
            if(status == '{!CARD_READ_STATUS_STARTED}'){
                $('input[id$="readCardDetailsBtn"]').prop('value', 'Reading Card Details...');
                $('input[id$="readCardDetailsBtn"]').prop("disabled", true).addClass('btnDisabled').removeClass('btn');
            }else{
                $('input[id$="readCardDetailsBtn"]').prop('value', 'Read Card Details');
                $('input[id$="readCardDetailsBtn"]').prop("disabled", false).removeClass('btnDisabled').addClass('btn');
            }
        }
        function DisplayEmiratesIdInfo(){
            var eid_elem;
            for (var k in card_details) {
                eid_elem = document.getElementById(k);
                if (eid_elem != undefined && card_details.hasOwnProperty(k)) {

                    if(k == 'photo_data'){
                        var base64 = ConvertToBase64(card_details[k]);
                        drawImage(base64, k);
                        console.log('Photo Data' + card_details[k]);
                    }
                    else {
                        eid_elem.innerHTML = "<b>"+card_details[k]+"</b>";
                    }

                }
            }
        }


        function toggleFieldsSelection(targetElem){

            var selectAll = targetElem.checked;
            var dataRows = document.getElementsByClassName('dataRow');
            for(var row =0; row < dataRows.length; row++) {
                var rowElem = dataRows[row];

                var rowCells = rowElem.children;
                if (rowCells.length == 4 && rowCells[2].children[0].disabled == false) {
                    rowCells[2].children[0].checked = selectAll;
                }
            }
        }


        function sync_update(){
            disableFields(false);

            var dataRows = document.getElementsByClassName('dataRow');

            // copy the rest fields
            for(var row =0; row < dataRows.length; row++){
                var rowElem = dataRows[row];

                var rowCells = rowElem.children;
                if(row == 0 && rowCells[1] && (rowCells.length == 4 && rowCells[2].children[0].checked)){
                    sync_photo();
                    continue;
                }


                if(rowCells.length == 4 && rowCells[2].children[0].checked){
                    // Find its child `input` elements

                    var new_val = (rowCells[1].innerText.trim() == '--' ? '' : rowCells[1].innerText.trim());

                    if($(rowCells).find('select').length > 0){

                        var sel_val = $(rowCells).find('select option').filter(
                                function () {
                                    return $(this).html() == new_val;
                                }
                        ).val();

                        $(rowCells[3]).find('select').val(sel_val);
                        console.log('selected Value >>' + sel_val);

                    }else if($(rowCells).find('input').length > 0){
                        $(rowCells[3]).find('input').val(new_val);
                    }
                }
            }

            //Sync card details
            syncCard();

            return false;
        }

        function sync_photo(){
            try {
                // copy the photography
                var photography_hexStr = card_details['photo_data'];
                if(photography_hexStr) {
                    var base64 = ConvertToBase64(photography_hexStr);
                    drawImage(base64, 'photoCanvas');

                    $(document.getElementById('{!$Component.emiratesForm.cardDetailsContainer.SRAccountPhoto}'))
                            .val(base64);
                    $(document.getElementById('{!$Component.emiratesForm.cardDetailsContainer.studentAccountPhoto}'))
                            .val(base64);

                }
            }catch (e){
                console.log('reading photography failed.' + e);
            }
        }


        function syncCard_Complete(){
            disableFields(true);
            DisplayEmiratesIdInfo();

            //clar all checkboxes
            var dataRows = document.getElementsByTagName('tr');
            for(var row =0; row < dataRows.length; row++) {
                var rowElem = dataRows[row];

                var rowCells = rowElem.children;
                if (rowCells.length == 4) {
                    //rowCells[2].children[0].checked = false;
                    $(rowCells[2]).find('input').prop('checked', false);
                }
            }

        }

        function onCancelClicked(){
            if('{!$CurrentPage.parameters.id}'.trim().length > 0)
                window.location.href= decodeURIComponent('/{!$CurrentPage.parameters.id}');
            else if('{!$CurrentPage.parameters.srid}'.trim().length > 0)
                window.location.href = decodeURIComponent('/{!$CurrentPage.parameters.srid}');
            else if('{!$CurrentPage.parameters.cancelUrl}'.trim().length > 0){
                window.location.href = decodeURIComponent('{!$CurrentPage.parameters.cancelUrl}');
            }else{
                window.location.href = '/home/home.jsp';
