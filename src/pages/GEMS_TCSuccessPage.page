<apex:page id="GEMS_TCSuccessPage" docType="html-5.0" showHeader="false">
<style>
    .pbSubsection {
        padding: 25px 0;
    }
</style>
    <apex:outputPanel styleClass="homeTab" layout="block">
        <apex:pageBlock >
            <apex:pageBlockSection collapsible="false" columns="1">
                <apex:outputPanel id="success" layout="block">
                    <table class="messageTable" style=" width: 100%; padding:0px;margin:0px;" cellspacing="0" cellpadding="0" border="0">
                        <tbody> 
                        <tr valign="top">
                            <!--<td width="35px"><img alt="CONFIRM" class="msgIcon" src="/img/msg_icons/confirm24.png" title="CONFIRM"/></td>-->
                            <td class="messageCell" style="vertical-align: middle">
                                <div id="GEMS_TCRequestPage:theForm:success" class="messageText">
                                <span id="GEMS_TCRequestPage">
                                     
                                </span>
                                    <b><apex:outputText value="{!$CurrentPage.parameters.msg}"/></b>
                                      
                                    <br/>
                                </div>
                            </td>
                        </tr><tr><td></td><td></td></tr>
                        </tbody>
                    </table>
                </apex:outputPanel>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:outputPanel>

    <script src="{!URLFOR($Resource.GEMS, 'modules/iframe-resizer/iframeResizer.contentWindow.min.js')}"></script>
</apex:page>